<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingStart="@dimen/dimen10dp"
    android:paddingEnd="@dimen/dimen10dp"
   >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_assessment_year"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Assessed Value 2022"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/tvAssessmentYearValue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp" />

    </LinearLayout>

    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Council Adjustment"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_council_adjustment_params"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Net Assessed Value"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_net_assessed_value"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Rate Payable"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_rate_payable_2022"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Discount(s) Applicable"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_discount_applicable"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Discounted Rate Payable"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_discounted_rate_payable"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/activityDetailsAssesmentHistory_tv_arrear"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="@string/assessment_amount"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/AssessmentAmount"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>



    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Rate Payable 2022"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_rate_payable"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtArrearValue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Arrears Due"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tvArrearValue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtPenalty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"

            android:layout_weight="1"
            android:text="@string/penalty"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tvPenaltyValue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtAmountPaid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Amount Paid"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tvAmountPaid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"
            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_due"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:text="Balance Due"
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tvDueValue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dimen10dp"
            android:layout_weight="1"
            android:gravity="end"

            android:text=""
            android:textColor="@color/navy_blue"
            android:textSize="13sp"
            />

    </LinearLayout>
</LinearLayout>

