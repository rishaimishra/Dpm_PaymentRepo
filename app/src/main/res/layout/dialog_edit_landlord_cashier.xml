                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_weight=".4"
                        android:text="Chiefdom"
                        android:textColor="@color/colorBlack" />


                    <EditText
                        android:id="@+id/edt_landlord_chiefdom"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_marginEnd="@dimen/dimen10dp"
                        android:layout_weight=".6"
                        android:background="@drawable/rounded_corner_solid_grey"
                        android:hint="Chiefdom"
                        android:padding="@dimen/dimen16dp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/headerTextSize14sp"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_weight=".4"
                        android:text="District"
                        android:textColor="@color/colorBlack" />


                    <EditText
                        android:id="@+id/edt_landlord_district"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_marginEnd="@dimen/dimen10dp"
                        android:layout_weight=".6"
                        android:background="@drawable/rounded_corner_solid_grey"
                        android:hint="District"
                        android:padding="@dimen/dimen16dp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/headerTextSize14sp"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_weight=".4"
                        android:text="Province"
                        android:textColor="@color/colorBlack" />


                    <EditText
                        android:id="@+id/edt_landlord_province"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_marginEnd="@dimen/dimen10dp"
                        android:layout_weight=".6"
                        android:background="@drawable/rounded_corner_solid_grey"
                        android:hint="Province"
                        android:padding="@dimen/dimen16dp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/headerTextSize14sp"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_weight=".4"
                        android:text="Post Code"
                        android:textColor="@color/colorBlack" />


                    <EditText
                        android:id="@+id/edt_landlord_postcode"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_marginEnd="@dimen/dimen10dp"
                        android:layout_weight=".6"
                        android:background="@drawable/rounded_corner_solid_grey"
                        android:hint="PostCode"
                        android:padding="@dimen/dimen16dp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/headerTextSize14sp"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_weight=".4"
                        android:text="Landlord Email"
                        android:textColor="@color/colorBlack" />


                    <EditText
                        android:id="@+id/edt_landlord_email"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:layout_marginTop="@dimen/dimen16dp"
                        android:layout_marginEnd="@dimen/dimen10dp"
                        android:layout_weight=".6"
                        android:background="@drawable/rounded_corner_solid_grey"
