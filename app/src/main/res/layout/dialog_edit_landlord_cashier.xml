                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="@dimen/dimen5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <ImageView
                            android:id="@+id/img_verification_document"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dimen120dp"
                            android:layout_gravity="center_horizontal"
                            android:adjustViewBounds="true"
                            android:scaleType="fitCenter"
                            android:src="@drawable/placeholder"

                            />
                        <!--    </androidx.cardview.widget.CardView>-->

                        <TextView
                            android:id="@+id/edt_note_id_proof"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:gravity="center"
                            android:paddingBottom="@dimen/dimen5dp"
                            android:text="Id proof"
                            android:textColor="@color/colorRed"
                            android:visibility="visible" />

                        <TextView

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen5dp"
                            android:gravity="center"
                            android:paddingBottom="@dimen/dimen5dp"
